vector<int> reverseArray(vector<int>& arr) {
    reverse(arr.begin(), arr.end());
    return arr;
}

int main() {
    vector<int> arr = {1, 4, 3, 2, 6, 5};
    arr = reverseArray(arr);
    for (int num : arr) {
        cout << num << " ";
    }
    cout << endl;  // Output: 5 6 2 3 4 1
    return 0;
